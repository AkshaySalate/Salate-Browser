            background-clip: text;
            -webkit-text-fill-color: transparent;
            animation: rainbowFlow 3s ease-in-out infinite,
                       textGlow 2s ease-in-out infinite alternate,
                       writeIn 4s ease-out forwards;
            text-shadow: 0 0 20px rgba(255, 255, 255, 0.3);
            letter-spacing: 3px;
        }

        .salate-text::before {
            content: 'Salate';
            position: absolute;
            top: 0;
            left: 0;
            background: linear-gradient(
                45deg,
                #ff0000, #ff8000, #ffff00, #80ff00,
                #00ff00, #00ff80, #00ffff, #0080ff,
                #0000ff, #8000ff, #ff00ff, #ff0080
            );
            background-size: 400% 400%;
            -webkit-background-clip: text;
            background-clip: text;
            -webkit-text-fill-color: transparent;
            animation: rainbowFlow 3s ease-in-out infinite reverse;
            filter: blur(1px);
            opacity: 0.7;
            z-index: -1;
        }

        .brush-stroke {
            position: absolute;
            top: 50%;
            left: 0;
            width: 100%;
            height: 8px;
            background: linear-gradient(
                90deg,
                transparent 0%,
                #ff6b6b 15%,
                #4ecdc4 35%,
                #45b7d1 50%,
                #96ceb4 65%,
                #feca57 85%,
                transparent 100%
            );
            border-radius: 50px;
            transform: translateY(-50%) scaleX(0);
            animation: brushStroke 3s ease-out 1s forwards;
            opacity: 0.6;
            filter: blur(0.5px);
        }

        .loading-dots {
            position: absolute;
            bottom: -50px;
            left: 50%;
            transform: translateX(-50%);
            display: flex;
            gap: 8px;
        }

        .dot {
            width: 12px;
            height: 12px;
            border-radius: 50%;
            background: linear-gradient(45deg, #ff6b6b, #4ecdc4);
            animation: loadingDots 1.5s ease-in-out infinite;
        }

        .dot:nth-child(2) { animation-delay: 0.2s; }
        .dot:nth-child(3) { animation-delay: 0.4s; }

        .sparkle {
            position: absolute;
            width: 6px;
            height: 6px;
            background: #fff;
            border-radius: 50%;
            animation: sparkle 2s ease-in-out infinite;
        }

        .sparkle:nth-child(1) {
            top: 20%;
            left: 15%;
            animation-delay: 0.5s;
        }

        .sparkle:nth-child(2) {
            top: 30%;
            right: 20%;
            animation-delay: 1.2s;
        }

        .sparkle:nth-child(3) {
            bottom: 25%;
            left: 25%;
            animation-delay: 2s;
        }

        .sparkle:nth-child(4) {
            top: 15%;
            right: 15%;
            animation-delay: 2.8s;
        }

        @keyframes rainbowFlow {
            0%, 100% { background-position: 0% 50%; }
            50% { background-position: 100% 50%; }
        }

        @keyframes textGlow {
            from {
                filter: drop-shadow(0 0 10px rgba(255, 255, 255, 0.3));
            }
            to {
                filter: drop-shadow(0 0 25px rgba(255, 255, 255, 0.6));
            }
        }

        @keyframes writeIn {
            0% {
                clip-path: inset(0 100% 0 0);
                transform: translateX(-20px);
                opacity: 0;
            }
            20% {
                opacity: 1;
            }
            100% {
                clip-path: inset(0 0 0 0);
                transform: translateX(0);
                opacity: 1;
            }
        }

        @keyframes brushStroke {
            0% {
                transform: translateY(-50%) scaleX(0);
                opacity: 0;
            }
            50% {
                opacity: 0.8;
            }
            100% {
                transform: translateY(-50%) scaleX(1);
                opacity: 0.4;
            }
        }

        @keyframes loadingDots {
            0%, 80%, 100% {
                transform: scale(0.8);
                opacity: 0.5;
            }
            40% {
                transform: scale(1.2);
                opacity: 1;
            }
        }

        @keyframes sparkle {
            0%, 100% {
                opacity: 0;
                transform: scale(0);
            }
            50% {
                opacity: 1;
                transform: scale(1);
            }
        }

        /* Floating particles */
        .particle {
            position: absolute;
            width: 4px;
            height: 4px;
            background: linear-gradient(45deg, #ff9a9e, #fecfef, #fecfef);
            border-radius: 50%;
            animation: float 4s ease-in-out infinite;
        }

        .particle:nth-child(1) {
            top: 10%;
            left: 10%;
            animation-delay: 0s;
        }

        .particle:nth-child(2) {
            top: 20%;
            right: 10%;
            animation-delay: 1s;
        }

        .particle:nth-child(3) {
            bottom: 20%;
            left: 20%;
            animation-delay: 2s;
        }

        @keyframes float {
            0%, 100% {
                transform: translateY(0px) rotate(0deg);
                opacity: 0.7;
            }
            50% {
                transform: translateY(-20px) rotate(180deg);
                opacity: 1;
            }
        }
